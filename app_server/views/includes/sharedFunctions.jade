mixin dateFormatter(dateString)
    -var date = new Date(dateString);
    -var d = date.getDate();
    -var m = date.getMonth();
    -var y = date.getFullYear();
    -var output = d+'/'+m+'/'+y;
    =output

mixin outputRating(rating)
    -for (var i = 1; i <= rating; i++)
     span.glyphicon.glyphicon-star
    -for (i = rating; i < 5 ; i++)
     apn.glyphicon.glyphicon-star-empty